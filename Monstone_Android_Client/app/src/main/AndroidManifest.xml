<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nightwingky.monstone">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".user_action.user_login.UserLoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".user_action.user_forget_pwd.UserForgetPwdActivity" />
        <activity android:name=".user_action.user_register.UserRegisterActivity" />
        <activity android:name=".main_program.MainFragmentActivity" />
        <activity android:name=".main_program.me.change_user_info.ChangeUserInfoActivity" />
        <activity android:name=".main_program.me.change_user_info.ItemChangeUserInfoActivity"></activity>
    </application>

</manifest>